  "engines": {
    "node": "10.16.0"
  },
  "dependencies": {
    "app-root-path": "^2.1.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "file-system": "^2.2.2",
    "mongodb": "^3.2.7",
    "morgan": "^1.9.1",
    "request": "^2.88.0",
